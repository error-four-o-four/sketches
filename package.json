{
  "name": "sketches",
  "type": "module",
  "version": "0.0.1",
  "workspaces": [
    "packages/*",
    "sketches/*",
    "templates/*"
  ],
  "scripts": {
    "format": "prettier --parser typescript \"((packages|sketches|templates)/**/*.ts|*.ts)\"",
    "format:fix": "npm run format -- --write",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "bin": {
    "create": "@internal/cli/create",
    "fetch": "@internal/cli/fetch"
  },
  "devDependencies": {
    "@internal/cdn": "^0.0.1",
    "@internal/cli": "^0.0.1",
    "@types/node": "^22.1.0",
    "@types/p5": "^1.7.6",
    "esbuild": "^0.23.0",
    "node-html-parser": "^6.1.13",
    "prettier": "^3.3.3",
    "vite": "^5.3.5"
  }
}
