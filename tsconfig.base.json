  "compilerOptions": {
		"target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",

    "skipLibCheck": true,
    "esModuleInterop": true,
		"lib": ["ESNext", "DOM"],

		"strict": true,
  },
  "exclude": [
    "**/node_modules",
    "node_modules",
    "public",
    "shared",
    "**/dist",
    "dist"
  ],
}
